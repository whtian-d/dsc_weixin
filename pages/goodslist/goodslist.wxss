@import "../../fonts/myfont/iconfont.wxss";
@import "../../fonts/dsc_small_font.wxss";
.goodslist_warp .search_warp{
    width: 100%;
    height: 94rpx;
    box-sizing: border-box;
    padding: 20rpx 20rpx 20rpx 40rpx;
    border-bottom: 1px solid #ccc;
    display: flex;
    justify-content: space-around;
    align-items: center;
}
.goodslist_warp .search_content{
    width: 580rpx;
    height: 100%;
    border-radius: 50rpx;
    border: 1px solid #ccc;
    position: relative;
    text-indent: 30rpx;
    font-size: 26rpx;
    line-height: 55rpx;
    color:#999;
}
.goodslist_warp .search_content .iconfont{
    position: absolute;
    top:50%;
    transform: translateY(-50%);
    right:30rpx;
}
/* 排序 */
.goodslist_warp .goods_tabs{
    width: 100%;
    height: 70rpx;
    padding: 0 20rpx;
    box-sizing: border-box;
    display: flex;
}
.goodslist_warp .goods_tabs .active{
    color:#f92028;
}
.goodslist_warp .goods_tabs .items_tab{
    width: 20%;
    height: 100%;
    display: flex;
    font-size: 26rpx;
    justify-content: center;
    align-items: center;
    overflow: hidden;
}
.goodslist_warp .goods_tabs .items_tab .iconfont{
    font-size: 24rpx;
    margin-top: 5rpx;
    margin-left: 5rpx;
    margin-right: 5rpx;
}
.goodslist_content{
    width: 100%;
    background-color: #efefef;
}
.goodslist_warp .bottominfo{
    width: 100%;
    display: flex;
    justify-content: center;
}